
/**
* Model for definition #definitions/{{&name}}
* Type: {{capitalize definition.type}}
*/

export class {{&name}} extends Base {
    refs () {
      return {
        {{#each definition.properties}}
          {{#propertyHasRef}}
            {{@key}}: {{propertyRef}}{{#unless @last}},{{/unless}}
          {{/propertyHasRef}}
        {{/each}}
      }
    }

    types () {
      return {
        {{#each definition.properties}}
            {{@key}}: "{{propertyType}}"{{#unless @last}},{{/unless}}
        {{/each}}
      }
    }

    defaults () {
        return {
            {{#each definition.properties}}
              {{@key}}: {{defaultPropertyValue}}{{#unless @last}},{{/unless}} {{defaultPropertyDescription}}
            {{/each}}
        }
    }
}
